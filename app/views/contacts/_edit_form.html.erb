
<%= form_for @contact do |f| %>


  <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </div>
 

 
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>

<%= link_to "Add Address", new_contact_address_path(@contact) %>
